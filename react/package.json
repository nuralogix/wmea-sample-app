{
  "name": "wmea-sample-app-react",
  "version": "0.1.0-beta.1",
  "description": "Nuralogix WMEA Sample App - React",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn watch\" \"yarn server:dev\"",
    "watch": "node --env-file=.dev.env ./node_modules/rollup/dist/bin/rollup --config ./config/rollup.config.mjs --watch",
    "build": "node --env-file=.prod.env ./node_modules/rollup/dist/bin/rollup --config ./config/rollup.config.mjs",
    "lint": "eslint",
    "typecheck:react": "tsc --noEmit --project tsconfig.json",
    "typecheck:server": "tsc --noEmit --project server/tsconfig.json",
    "typecheck": "yarn typecheck:react && yarn typecheck:server",
    "server:dev": "tsx --env-file=.dev.env server/src/index.ts",
    "server:prod": "NODE_ENV=production tsx --env-file=.prod.env server/src/index.ts"
  },
  "keywords": [
    "Nuralogix",
    "WMEA Sample App",
    "React",
    "Web measurement embedded app"
  ],
  "license": "SEE LICENSE IN ../LICENSE",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@nuralogix.ai/web-measurement-embedded-app": "^0.1.0-beta.2",
    "@nuralogix.ai/web-ui": "^0.1.0-alpha.13",
    "@stylexjs/stylex": "0.9.3",
    "@nuralogix.ai/dfx-api-client": "^1.0.73",
    "compression": "^1.8.0",
    "connect-livereload": "^0.6.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "i18next": "^25.3.2",
    "i18next-fetch-backend": "^6.0.0",
    "livereload": "^0.9.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-router": "^7.8.0",
    "react-router-dom": "^7.8.0",
    "valtio": "^2.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/core": "^7.28.0",
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@stylexjs/rollup-plugin": "^0.13.1",
    "@types/compression": "^1.8.1",
    "@types/connect-livereload": "^0.6.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/livereload": "^0.9.5",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/parser": "^8.25.0",
    "@typescript-eslint/types": "^8.25.0",
    "concurrently": "^9.1.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-valtio": "^0.8.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "rollup": "^4.44.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-delete": "^3.0.0",
    "rollup-plugin-esbuild": "^6.2.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.25.0"
  },
  "browserslist": [
    "Safari >= 18",
    "last 3 versions"
  ]
}
